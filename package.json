{
  "name": "pixi-live2d-display",
  "version": "0.1.2",
  "description": "Live2D plugin for PixiJS",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/guansss/pixi-live2d-display.git"
  },
  "author": "guansss <821143943@qq.com>",
  "license": "MIT",
  "scripts": {
    "pretest": "webpack --config webpack.test.config.js",
    "test": "node node_modules/floss/bin/floss.js --path build.test -d",
    "build": "webpack --mode=production && yarn type",
    "type": "yarn type:tsc && yarn type:dts",
    "type:tsc": "tsc --emitDeclarationOnly",
    "type:dts": "dts-bundle --name pixi-live2d-display --main build/index.d.ts --out ~/lib/index.d.ts",
    "prepublish": "yarn build"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {

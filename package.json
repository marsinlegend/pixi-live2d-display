{
  "name": "pixi-live2d-display",
  "version": "0.2.0",
  "description": "Live2D plugin for PixiJS",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/guansss/pixi-live2d-display.git"
  },
  "author": "guansss <821143943@qq.com>",
  "license": "MIT",
  "scripts": {
    "pretest": "webpack --config webpack.test.config.js",
    "test": "node node_modules/floss/bin/floss.js --path build.test -d",
    "build": "webpack --mode=production",
    "type": "dts-bundle-generator --no-check -o types/index.d.ts src/index.ts && node scripts/patch-types.js",
    "doc": "rimraf docs && typedoc src --mode file --excludeNotExported && node scripts/patch-docs.js",
    "prepack": "yarn build && yarn type",
    "prepublish": "yarn doc"
  },
  "files": [
    "lib",
    "src",
    "types"
  ],
  "types": "types/index.d.ts",
  "dependencies": {
    "lodash": "latest"
  },
  "peerDependencies": {
    "@pixi/core": "^5.2.1",
    "@pixi/display": "^5.2.1",
    "@pixi/loaders": "^5.2.1",
    "@pixi/math": "^5.2.1",
    "@pixi/sprite": "^5.2.1",
    "@pixi/utils": "^5.2.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "latest",
    "chalk": "^4.0.0",
    "dts-bundle-generator": "^4.0.0",
    "electron": "^8.1.1",
    "floss": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "glob": "^7.1.6",
    "lodash": "latest",
    "pixi.js": "^5.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}

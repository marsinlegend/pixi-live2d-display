    "@pixi/display": "^5.2.1",
    "@pixi/loaders": "^5.2.1",
    "@pixi/math": "^5.2.1",
    "@pixi/sprite": "^5.2.1",
    "@pixi/utils": "^5.2.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "latest",
    "dts-bundle": "^0.7.3",
    "electron": "^8.1.1",
    "floss": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "lodash": "latest",
    "pixi.js": "^5.2.1",
    "sinon": "^9.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
